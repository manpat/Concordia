<?xml version="1.0" encoding="UTF-8"?>
<Window id="bbgame_flowers_debug" title="game.flowers.base AnimDebug" resize="false" top="10" left="10" min-width="300" min-height="200">
  <Box orientation="vertical">
    <Label id="bbgame_flowers_anim_id"></Label>
    <Box>
      <Label class="bb_green">Current Frame:</Label>
      <Label id="bbgame_flowers_anim_current_frame">0.0</Label>
    </Box>
    <Box>
      <Label class="bb_green">Maximum Frames:</Label>
      <Label id="bbgame_flowers_anim_max_frames">0.0</Label>
    </Box>
    <Box>
      <Button id="bbgame_flowers_step_prev" expand="false" fill="false">&lt;</Button>
      <Scale id="bbgame_flowers_frame_setting" min="0.0" max="240.0"></Scale>
      <Button id="bbgame_flowers_step_next" expand="false" fill="false">&gt;</Button>
    </Box>
    <Box>
      <Label>Set Frame</Label>
      <Entry id="bbgame_flowers_anim_set_input"></Entry>
      <Button id="bbgame_flowers_anim_set_input_button">Set</Button>
    </Box>
  </Box>
</Window>
